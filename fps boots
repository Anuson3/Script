-- TELEGUIADO SEGURO
local savedCFrame

saveTpBtn.MouseButton1Click:Connect(function()
    local char = player.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        savedCFrame = char.HumanoidRootPart.CFrame
    end
end)

goTpBtn.MouseButton1Click:Connect(function()
    if not savedCFrame then return end
    local char = player.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    local humanoid = char and char:FindFirstChildOfClass("Humanoid")
    if hrp and humanoid then
        humanoid.PlatformStand = true
        local tween = TweenService:Create(hrp, TweenInfo.new(0.15, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {CFrame = savedCFrame})
        tween:Play()
        tween.Completed:Wait()
        humanoid.PlatformStand = false
    end
end)

-- ABRIR/FECHAR GUI
local open = false
openButton.MouseButton1Click:Connect(function()
    open = not open
    mainFrame.Visible = open
end)

-- BOT√ïES PRINCIPAIS
boostButton.MouseButton1Click:Connect(fpsBoost)
disableButton.MouseButton1Click:Connect(disableFpsBoost)
stretchButton.MouseButton1Click:Connect(toggleStretch)
helpStealButton.MouseButton1Click:Connect(function()
    helpInterface.Visible = not helpInterface.Visible
end)
