local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

print("FPSBoost+ LocalScript iniciado")

--// ‡πÇ‡∏Ñ‡πâ‡∏î GUI ‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï‡πÅ‡∏ö‡∏ö‡∏°‡∏µ Tween ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏Å‡∏•‡∏≤‡∏á-‡∏≠‡∏≠‡∏Å (‡∏õ‡∏£‡∏±‡∏ö‡∏õ‡∏£‡∏∏‡∏á‡πÉ‡∏´‡∏°‡πà)
local CreditScreenGui = Instance.new("ScreenGui")
CreditScreenGui.Name = "FPSBoosterCreditGUI"
CreditScreenGui.Parent = game.CoreGui -- ‡πÉ‡∏ä‡πâ CoreGui ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•‡∏Å‡πà‡∏≠‡∏ô PlayerGui

local CreditFrame = Instance.new("Frame")
CreditFrame.Name = "CreditFrame"
CreditFrame.Size = UDim2.new(0, 400, 0, 150) -- ‡∏Ç‡∏ô‡∏≤‡∏î‡πÄ‡∏ü‡∏£‡∏°‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï
CreditFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
CreditFrame.BackgroundTransparency = 0.1
CreditFrame.BorderSizePixel = 0
CreditFrame.Parent = CreditScreenGui

local CreditTextLabel = Instance.new("TextLabel")
CreditTextLabel.Name = "CreditText"
CreditTextLabel.Text = "FPS Boost+ Activated\nby ttk: breaksstore" -- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï
CreditTextLabel.Size = UDim2.new(1, 0, 1, 0)
CreditTextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
CreditTextLabel.TextStrokeTransparency = 0.5
CreditTextLabel.BackgroundTransparency = 1
CreditTextLabel.TextScaled = true
CreditTextLabel.Font = Enum.Font.GothamBold
CreditTextLabel.Parent = CreditFrame

--// ‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏Å‡∏•‡∏≤‡∏á-‡∏≠‡∏≠‡∏Å
task.spawn(function()
    -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
    local centerPos = UDim2.new(0.5, -200, 0.5, -75) -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠ (0.5, -halfWidth, 0.5, -halfHeight)
    local startPos = UDim2.new(0, -400, 0.5, -75) -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏ô‡∏≠‡∏Å‡∏à‡∏≠‡∏ó‡∏≤‡∏á‡∏ã‡πâ‡∏≤‡∏¢
    local endPos = UDim2.new(1, 0, 0.5, -75) -- ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢‡∏ô‡∏≠‡∏Å‡∏à‡∏≠‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤

    CreditFrame.Position = startPos -- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô

    -- ‡∏Å‡∏≤‡∏£‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Tween
    local slideInfo = TweenInfo.new(
        1, -- ‡πÄ‡∏ß‡∏•‡∏≤ 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏î‡∏π "‡∏Ñ‡πà‡∏≠‡∏¢‡πÜ" ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô
        Enum.EasingStyle.Quad, -- ‡∏™‡πÑ‡∏ï‡∏•‡πå‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß
        Enum.EasingDirection.Out -- ‡∏ó‡∏¥‡∏®‡∏ó‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÑ‡∏´‡∏ß
    )

    -- 1. ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏Å‡∏•‡∏≤‡∏á‡∏à‡∏≠
    local slideInTween = TweenService:Create(CreditFrame, slideInfo, {Position = centerPos})
    slideInTween:Play()
    slideInTween.Completed:Wait()

    -- 2. ‡∏£‡∏≠ 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    task.wait(2)

    -- 3. ‡πÄ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏≠‡∏≠‡∏Å‡πÑ‡∏õ‡∏ó‡∏≤‡∏á‡∏Ç‡∏ß‡∏≤
    local slideOutTween = TweenService:Create(CreditFrame, slideInfo, {Position = endPos})
    slideOutTween:Play()
    slideOutTween.Completed:Wait()

    -- ‡∏ó‡∏≥‡∏•‡∏≤‡∏¢ GUI
    CreditScreenGui:Destroy()
end)
--// ‡∏™‡∏¥‡πâ‡∏ô‡∏™‡∏∏‡∏î‡πÇ‡∏Ñ‡πâ‡∏î GUI ‡πÄ‡∏Ñ‡∏£‡∏î‡∏¥‡∏ï

